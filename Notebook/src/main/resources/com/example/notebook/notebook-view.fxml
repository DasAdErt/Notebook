<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.notebook.NotebookController">
   <children>
      <ListView fx:id="listView" layoutX="300.0" layoutY="27.0" prefHeight="344.0" prefWidth="278.0" />
      <Label layoutX="92.0" layoutY="27.0" text="Notebook">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <TextField fx:id="inputField" layoutX="119.0" layoutY="88.0" />
      <Label layoutX="45.0" layoutY="85.0" text="Input">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Button fx:id="buttonAdd" layoutX="66.0" layoutY="138.0" mnemonicParsing="false" onAction="#buttonClickAdd" text="Add">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="buttonDelete" layoutX="200.0" layoutY="138.0" mnemonicParsing="false" onAction="#buttonClickDelete" text="Delete">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Label fx:id="computerNameLabel" layoutX="120.0" layoutY="248.0" text="username" AnchorPane.leftAnchor="120.0">
         <font>
            <Font size="18.0" />
         </font></Label>
      <ImageView fx:id="avatarImageView" fitHeight="40.0" fitWidth="40.0" layoutX="70.0" layoutY="241.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
